<%= render 'shared/header' %>
<div class='container'>
  <div class='content'>
    <%= render 'shared/sidebar' %>
    <div class='preamble'>
      <h1>Recommendations</h1>
    </div>

    <ul>
      <h2>Select a visitor:</h2>
      <% @visitors.each do |visitor| %>
        <li><%= link_to visitor.name, visitor %></li>
      <% end %>
    </ul>

  </div>
  <%= render 'shared/footer' %>
</div>
